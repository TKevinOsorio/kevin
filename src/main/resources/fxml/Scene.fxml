<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.web.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="444.0" prefWidth="305.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cg.app.proyectopersonal.FXMLController">
   <children>
      <MenuBar layoutY="1.0" prefHeight="27.0" prefWidth="305.0">
        <menus>
          <Menu mnemonicParsing="false" text="Archivo">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#mostrarDatos" text="Mostrar" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" onAction="#salir" text="Salir" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Ordenamiento">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#ordenamientoEdad" text="Por edad" />
                  <MenuItem mnemonicParsing="false" onAction="#ordenamientoIdentificacion" text="Por identificacion" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Ayuda">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#creditos" text="Creditos" />
            </items>
          </Menu>
        </menus>
         <effect>
            <Lighting diffuseConstant="1.84">
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant color="#c4a3a3" />
               </light>
            </Lighting>
         </effect>
      </MenuBar>
      <WebView fx:id="webView" layoutY="28.0" prefHeight="311.0" prefWidth="305.0">
         <cursor>
            <Cursor fx:constant="DISAPPEAR" />
         </cursor></WebView>
      <Button layoutX="14.0" layoutY="348.0" mnemonicParsing="false" onAction="#mostrarDatosBoton" prefHeight="89.0" prefWidth="104.0" text="Mostrar Datos">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Lighting diffuseConstant="1.37">
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant color="RED" />
               </light>
            </Lighting>
         </effect></Button>
      <Button layoutX="146.0" layoutY="393.0" mnemonicParsing="false" onAction="#organizarDatosIdBoton" prefHeight="43.0" prefWidth="136.0" text="Organizar por ID">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Lighting diffuseConstant="1.74">
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant color="#7300fa" />
               </light>
            </Lighting>
         </effect></Button>
      <Button layoutX="146.0" layoutY="348.0" mnemonicParsing="false" onAction="#organizarDatosEdad" prefHeight="43.0" prefWidth="136.0" text="Organizar por Edad" textAlignment="JUSTIFY">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Lighting diffuseConstant="2.0">
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant color="#15ebd2" />
               </light>
            </Lighting>
         </effect></Button>
   </children>
   <effect>
      <InnerShadow color="#05ff8d" />
   </effect>
</AnchorPane>
